<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="RayCast" main="RayCast"
		package="com.matttuttle.raycast" version="1.0.0" company="HaxePunk" />
	<window width="640" height="400" fps="30"
		orientation="portrait" resizable="true" />

	<set name="BUILD_DIR" value="bin" />

	<classpath name="src" />
	<haxelib name="nme" />

	<assets path="assets" include="*.png" type="image" />

	<ndll name="std" />
	<ndll name="regexp" />
	<ndll name="zlib" />
	<ndll name="nme" haxelib="nme" />
</project>
